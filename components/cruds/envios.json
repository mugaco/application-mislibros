{
  "resource": "Quartup\\Filegun\\Models\\Document",
  "appBar": false,
  "addItem": true,
  "drawerCustomComponent": "envios/app-enviar-documentos.vue",
  "excel": false,
  "table": {
    "title": "envios",
    "simpleSearch": true,
    "searchBy": [
      "doc_original_name",
      "message",
      "to_name",
      "to_email",
      "names"
    ],
    "permanent": {
      "user_id": "store-state-user-user_id"
    },
    "aggregation": [
      {
        "project": {
          "doc_original_name": 1,
          "to_name": 1,
          "to_email": 1,
          "names": 1,
          "url": 1,
          "downloaded": 1,
          "created_at": 1,
          "updated_at": 1,
          "hash": 1,
          "is_zip": 1,
          "metadata": 1,
          "message": 1
        },
        "sort": {
          "created_at": -1
        }
      }
    ],
    "headers": [
      {
        "text": "archivo",
        "value": "doc_original_name",
        "sortable": true,
        "type": "menu",
        "items": "names"
      },
      {
        "text": "contacta-mensaje",
        "value": "message",
        "sortable": false,
        "type": "menu"
      },
      {
        "text": "nombre-destinatario",
        "value": "to_name",
        "sortable": true
      },
      {
        "text": "email-destinatario",
        "value": "to_email",
        "sortable": true
      },
     
      {
        "text": "Link",
        "value": "url",
        "sortable": false,
        "type": "copy",
        "limit": 5
      },
      {
        "text": "enviado",
        "value": "created_at",
        "sortable": true,
        "type": "date"
      },
      {
        "text": "descargado",
        "value": "updated_at",
        "sortable": true,
        "type": "date",
        "if":"downloaded"
      }
    ]
  },
  "filters": {
    "model": {
      "downloaded": null,
      "updated_at": null,
      "created_at": null,
      "hash": null
    },
    "form": [
      {
        "component": "select",
        "props": {
          "key": "downloaded",
          "items": [
            {
              "value": null,
              "text": "Todos"
            },
            {
              "value": [
                true
              ],
              "text": "Solo descargados"
            },
            {
              "value": [
                null
              ],
              "text": "No descargados"
            }
          ],
          "itemValue": "value",
          "itemText": "text",
          "smallChips": true,
          "label": "Mostrar descargados"
        }
      },
      {
        "component": "date-picker-range",
        "props": {
          "key": "created_at",
          "label": "enviado",
          "cols": {
            "cols": 12,
            "md": 6,
            "sm": 3
          }
        }
      } ,
      {
        "component": "hash",
        "props": {
          "key": "hash",
          "label": "Busca hash",
          "cols": {
            "cols": 12,
            "md": 6,
            "sm": 3
          }
        }
      }
    ]
  }
}